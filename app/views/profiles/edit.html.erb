<h1>プロフィール編集画面</h1>
<%= form_for current_user, url: {action: 'update'} do |f| %>
 
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
 
  <div class="actions">
    <%= f.submit "Update" %>
  </div>
<% end %>
<p>Find me in app/views/profiles/edit.html.erb</p>
